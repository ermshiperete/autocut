	"version": "0.2.0",
	"configurations": [

		{
			"name": "Python: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"justMyCode": true
		},
		{
			"name": "Python: autocut",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/autocut.py",
			// "args": ["--debug", "--no-preconvert", "--no-upload", "--end-intro", "20"],
			"args": ["--debug", "--no-preconvert", "--no-intro-detection", "--no-upload"],
			"console": "integratedTerminal",
			"justMyCode": true
		}
	]
}